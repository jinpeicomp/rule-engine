package com.jinpei.re.dynamic

import com.jinpei.re.core.model.UserAttribute
import java.math.BigDecimal
import com.jinpei.re.common.ReUtils

dialect "java"

/**
* ${name}-${id}
* @author Mingo.Liu
*/

global java.util.List<Long> list

rule "${name}"
    salience ${weight}
    when
        UserAttribute(${lhs});
    then
        list.add(${id});
        ReUtils.printRule("${name}-${id}");
end